{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ],
    "prettier/prettier": "error",
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "no-nested-ternary": "off"
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
